<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="de.blinkt.xp.openvpn">

    <application tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name="de.blinkt.openvpn.activities.NotImplemented"
            tools:ignore="ExportedActivity" />

        <activity-alias
                android:name="de.blinkt.openvpn.activities.LogActivity"
                android:targetActivity="de.blinkt.openvpn.activities.NotImplemented"
            />

        <activity-alias
                android:name="de.blinkt.openvpn.activities.Req"
                android:targetActivity="de.blinkt.openvpn.activities.NotImplemented"
                />

    </application>

</manifest>
